{
  "name": "webdriverio-automation",
  "version": "1.0.0",
  "description": "Automation framework to test web application,API and mobile app",
  "main": "index.js",
  "scripts": {
    "start": "node sendEmailNotification.mjs",
    "div1": "==========================Web Local=========================================================",
    "smoke_suites": "set HEADLESS=N && npx wdio wdio.conf.ts --cucumberOpts.tagExpression='@smoke'",
    "regression_suites": "set HEADLESS=N &&npx wdio wdio.conf.ts --cucumberOpts.tagExpression='@regression'",
    "E2ETest": "set HEADLESS=N && npx wdio wdio.conf.ts --cucumberOpts.tagExpression='@e2e'",
    "dev:hrmsLogin": "set HEADLESS=N && npx wdio wdio.conf.ts --cucumberOpts.tagExpression='@hrms'",
    "dev:productStore": "set HEADLESS=Y && npx wdio wdio.conf.ts --cucumberOpts.tagExpression='@product_store'",
    "div2": "==========================Mobile Local=========================================================",
    "android-mobile-e2e": "set HEADLESS=N && npx wdio ./config/mobile-local-config.ts --cucumberOpts.tagExpression='@mobile'",
    "div3": "==========================Web Sauce Test=========================================================",
    "test-web-sauce-emulator": "set HEADLESS=N && npx wdio ./config/web-sauce-lab-config.ts --cucumberOpts.tagExpression='@product_store'",
    "div4": "==========================Mobile Sauce Test=========================================================",
    "test-mobile-sauce-emulator": "set HEADLESS=N && npx wdio ./config/mobile-sauce-lab-config.ts --cucumberOpts.tagExpression='@mobile'",
    "div5": "==========================API Test=========================================================",
    "api_test_e2e": "set HEADLESS=Y && npx wdio wdio.conf.ts --cucumberOpts.tagExpression='@api'",
    "div6": "==========================Report=========================================================",
    "test": "wdio wdio.conf.js --reporters allure",
    "reports": "allure generate allure-results --clean  && allure open"
  },
  "keywords": [
    "wdio",
    "cucumber",
    "e2e",
    "tests"
  ],
  "author": "Vikas Kumar",
  "license": "ISC",
  "devDependencies": {
    "@appium/logger": "^1.1.0",
    "@applitools/eyes-sdk-core": "^13.11.34",
    "@cucumber/cucumber": "^10.8.0",
    "@eslint/compat": "^1.0.3",
    "@eslint/js": "^9.4.0",
    "@playwright/test": "^1.24.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/chai": "^4.3.1",
    "@types/jquery": "^3.5.29",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/types": "^7.6.0",
    "@wdio/allure-reporter": "^7.20.3",
    "@wdio/appium-service": "^8.38.2",
    "@wdio/cli": "^7.20.5",
    "@wdio/cucumber-framework": "^7.20.3",
    "@wdio/globals": "^8.33.1",
    "@wdio/junit-reporter": "7",
    "@wdio/local-runner": "^7.20.5",
    "@wdio/sauce-service": "^8.36.1",
    "@wdio/spec-reporter": "^7.20.3",
    "@wdio/sync": "^7.27.0",
    "@wdio/utils": "^8.35.0",
    "@wdio/visual-service": "^4.1.0",
    "allure-commandline": "^2.29.0",
    "app-root-path": "^3.0.0",
    "appium": "^2.7.0",
    "appium-doctor": "^1.16.2",
    "appium-uiautomator2-driver": "^3.0.6",
    "archiver": "^7.0.1",
    "chai": "^4.3.6",
    "chromedriver": "^125.0.1",
    "csv-parse": "^5.5.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "geckodriver": "^3.0.2",
    "get-stream": "^6.0.1",
    "globals": "^15.3.0",
    "i": "^0.3.7",
    "mailosaur": "^8.4.0",
    "nodemailer": "^6.9.13",
    "npm": "^10.5.0",
    "parse": "^4.3.1",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.4",
    "typescript-eslint": "^7.12.0",
    "wd": "^1.14.0",
    "wdio-chromedriver-service": "^7.3.2",
    "wdio-geckodriver-service": "^2.1.2",
    "winston": "^3.12.0",
    "xlsx": "^0.18.5"
  }
}
