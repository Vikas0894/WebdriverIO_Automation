{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "chrome",
        "url": "https://www.demoblaze.com/",
        "webRoot": "${workspaceFolder}"
      },
      

      {
        "type": "node",
        "request": "launch",
        "name": "Debug WebDriverIO Test",
        "skipFiles": ["<node_internals>/**"],
        "program": "../wdio.conf.ts",
        "args": ["run", "wdio.conf.ts"],
        "autoAttachChildProcesses": true,
        "sourceMaps": true
      }
    ]
  }
  